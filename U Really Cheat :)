local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()


-- Stealth Execution untuk menyamarkan skrip
local function stealthExec(func)
    return coroutine.wrap(func)()
end

local player = game.Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local hum = char:FindFirstChildOfClass("Humanoid")

-- Bypass WalkSpeed & JumpPower (Memory Editing)
local function memoryBypass(property, value)
    local success, err = pcall(function()
        if sethiddenproperty then
            sethiddenproperty(hum, property, value)
        elseif setfflag then
            setfflag("Humanoid" .. property, tostring(value))
        end
    end)
    return success
end

-- Bypass Fly menggunakan method baru
local function bypassFly()
    local hrp = char:FindFirstChild("HumanoidRootPart")
    if not hrp then return end

    local bp = Instance.new("BodyPosition", hrp)
    bp.Position = hrp.Position + Vector3.new(0, 50, 0)
    bp.MaxForce = Vector3.new(9e9, 9e9, 9e9)

    wait(3)
    bp:Destroy()
end

-- Bypass Remote Validation
local function hijackRemote()
    for _, v in pairs(getgc(true)) do
        if type(v) == "function" and islclosure(v) and not is_synapse_function(v) then
            local env = getfenv(v)
            if env and env.script and env.script:IsA("LocalScript") then
                setupvalue(v, 1, function(...) return true end)
            end
        end
    end
end

-- Disable Anti-Cheat Hooks
local function disableDetections()
    for _, v in pairs(getgc(true)) do
        if type(v) == "function" and islclosure(v) then
            if not is_synapse_function(v) then
                hookfunction(v, function(...)
                    return wait(9e9) -- Menghentikan fungsi deteksi
                end)
            end
        end
    end
end

-- Eksekusi semua bypass
stealthExec(function()
    memoryBypass("WalkSpeed", 100)
    memoryBypass("JumpPower", 100)
    bypassFly()
    hijackRemote()
    disableDetections()
end)


local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local hum = char:FindFirstChildOfClass("Humanoid")

-- ðŸ›¡ï¸ Stealth Execution (Menyembunyikan eksekusi dari anti-cheat)
local function stealthExec(func)
    return task.spawn(function()
        local success, err = pcall(func)
        if not success then
            warn("Stealth Error:", err)
        end
    end)
end

-- ðŸ”„ Optimized Loop (Mengurangi Lag & Bypass Detection)
local function optimizedLoop(callback)
    local lastTick = tick()
    local function loop()
        while true do
            task.wait(0.1) -- Hindari penggunaan CPU berlebih
            if tick() - lastTick > 5 then -- Cegah deteksi script injection
                lastTick = tick()
                pcall(callback)
            end
        end
    end
    stealthExec(loop)
end

-- ðŸ—‘ï¸ Custom Garbage Collector (Menghapus objek tanpa deteksi)
local function safeDestroy(object)
    if object and object.Parent then
        object.Parent = nil
        task.wait(0.1) -- Delay kecil agar tidak mencurigakan
        object:Destroy()
    end
end

-- ðŸ” Bypass Anti-Cheat (Bypass perubahan nilai Humanoid)
optimizedLoop(function()
    if hum then
        pcall(function()
            sethiddenproperty(hum, "WalkSpeed", 100) -- Menghindari deteksi WalkSpeed
            sethiddenproperty(hum, "JumpPower", 100) -- Menghindari deteksi JumpPower
        end)
    end
end)

-- ðŸ”„ Auto-Reapply Setelah Respawn
player.CharacterAdded:Connect(function(newChar)
    char = newChar
    hum = char:FindFirstChildOfClass("Humanoid")
    stealthExec(function()
        task.wait(1)
        if hum then
            sethiddenproperty(hum, "WalkSpeed", 100)
            sethiddenproperty(hum, "JumpPower", 100)
        end
    end)
end)


