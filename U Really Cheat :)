local player = game.Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local hrp = char:WaitForChild("HumanoidRootPart")

local isFlying = false
local flySpeed = 50
local bodyVelocity

-- Fungsi untuk memulai fly
local function startFly()
    if isFlying and hrp then
        bodyVelocity = Instance.new("BodyVelocity", hrp)
        bodyVelocity.Velocity = Vector3.new(0, flySpeed, 0)
        bodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)
    end
end

-- Fungsi untuk menghentikan fly
local function stopFly()
    if bodyVelocity then
        bodyVelocity:Destroy()
        bodyVelocity = nil
    end
end

-- Toggle Fly
local FlyToggle = TabMisc:CreateToggle({
    Name = "Fly",
    CurrentValue = false,
    Flag = "FlyToggle",
    Callback = function(Value)
        isFlying = Value
        if isFlying then
            startFly()
        else
            stopFly()
        end
    end
})

-- Slider Speed
local FlySlider = TabMisc:CreateSlider({
    Name = "Fly Speed",
    Range = {10, 200},
    Increment = 5,
    Suffix = "Speed",
    CurrentValue = flySpeed,
    Flag = "FlySlider",
    Callback = function(Value)
        flySpeed = Value
        if isFlying and bodyVelocity then
            bodyVelocity.Velocity = Vector3.new(0, flySpeed, 0)
        end
    end
})

-- Auto-enable fly setelah respawn
player.CharacterAdded:Connect(function(newChar)
    char = newChar
    hrp = char:WaitForChild("HumanoidRootPart")
    
    if isFlying then
        wait(1) -- Tunggu karakter benar-benar respawn
        startFly()
    end
end)


-- Variabel Gravitasi
local gravityEnabled = false
local gravityValue = 196.2 -- Nilai default gravitasi Roblox

-- Fungsi untuk Mengatur Gravitasi
local function SetGravity(value)
    game.Workspace.Gravity = value
end

-- Toggle untuk Mengaktifkan/Mematikan Pengaturan Gravitasi
TabMisc:CreateToggle({
    Name = "Gravity",
    CurrentValue = false,
    Flag = "Gravity Moon",
    Callback = function(value)
        gravityEnabled = value
        if not gravityEnabled then
            -- Mengembalikan gravitasi ke default saat toggle dimatikan
            SetGravity(196.2)
        else
            SetGravity(gravityValue)
        end
    end
})

-- Slider untuk Mengatur Gravitasi dari 1000 ke 0
TabMisc:CreateSlider({
    Name = "Gravity Level",
    Range = {0, 1000},
    Increment = 10,
    Suffix = "Gravity",
    CurrentValue = gravityValue,
    Flag = "GravitySlider",
    Callback = function(value)
        gravityValue = value
        if gravityEnabled then
            SetGravity(gravityValue)
        end
    end
})


-- Variabel Fling
local flingEnabled = false
local flingPower = 100000 -- Nilai default kekuatan fling

-- Fungsi untuk Melakukan Fling
local function FlingCharacter()
    local plr = game.Players.LocalPlayer
    local char = plr.Character or plr.CharacterAdded:Wait()
    local hrp = char:FindFirstChild("HumanoidRootPart")
    
    if not hrp then return end

    local bv = Instance.new("BodyVelocity")
    bv.Velocity = Vector3.new(math.random(-flingPower, flingPower), flingPower, math.random(-flingPower, flingPower))
    bv.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
    bv.Parent = hrp

    task.wait(0.1) -- Memberikan sedikit waktu untuk fling berfungsi
    bv:Destroy() -- Menghapus BodyVelocity setelah fling agar tidak terus berlanjut
end

-- Toggle untuk Mengaktifkan/Mematikan Fling
TabMisc:CreateToggle({
    Name = "Fling",
    CurrentValue = false,
    Flag = "Fling Mode",
    Callback = function(value)
        flingEnabled = value
        if flingEnabled then
            FlingCharacter()
        end
    end
})

-- Slider untuk Mengatur Kekuatan Fling
TabMisc:CreateSlider({
    Name = "Fling Power",
    Range = {1000, 1000000},
    Increment = 1000000,
    Suffix = "Power",
    CurrentValue = flingPower,
    Flag = "FlingPower",
    Callback = function(value)
        flingPower = value
    end
})


-- Variables
local spinning = false
local spinSpeed = 10

-- Function to start spinning
local function startSpinning()
    while spinning do
        local character = game.Players.LocalPlayer.Character
        if character and character:FindFirstChild("HumanoidRootPart") then
            character.HumanoidRootPart.CFrame *= CFrame.Angles(0, math.rad(spinSpeed), 0)
        end
        task.wait(0.1) -- Adjust loop execution speed
    end
end

-- Toggle for Spin
TabMisc:CreateToggle({
    Name = "Enable Spin",
    CurrentValue = false,
    Flag = "SpinToggle",
    Callback = function(value)
        spinning = value
        if spinning then
            startSpinning()
        end
    end
})

-- Slider for Spin Speed
TabMisc:CreateSlider({
    Name = "Spin Speed",
    Range = {1, 100},
    Increment = 1,
    Suffix = "Â°/frame",
    CurrentValue = 10,
    Flag = "SpinSpeed",
    Callback = function(value)
        spinSpeed = value
    end
})


-- Variables
local noClipEnabled = false

-- Function to enable/disable NoClip
local function setNoClip(state)
    noClipEnabled = state
    local player = game.Players.LocalPlayer

    while noClipEnabled do
        if player and player.Character then
            for _, part in pairs(player.Character:GetDescendants()) do
                if part:IsA("BasePart") and part.CanCollide then
                    part.CanCollide = false
                end
            end
        end
        task.wait()
    end

    -- Re-enable collisions when NoClip is turned off
    if player and player.Character then
        for _, part in pairs(player.Character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = true
            end
        end
    end
end

-- Toggle for NoClip
TabMisc:CreateToggle({
    Name = "Enable NoClip",
    CurrentValue = false,
    Flag = "NoClipToggle",
    Callback = function(value)
        setNoClip(value)
    end
})


local player = game.Players.LocalPlayer
local camera = game.Workspace.CurrentCamera
local espEnabled = false
local selectedPlayer = "All"
local espStyle = "Box" -- Default bentuk ESP
local objectsEspEnabled = false

local espFolder = Instance.new("Folder", game.CoreGui)
espFolder.Name = "ESPFolder"

-- Fungsi untuk menggambar ESP
local function createEsp(target)
    if target == player.Character then return end -- Jangan ESP diri sendiri
    
    local espBox = Drawing.new("Square")
    local espLine = Drawing.new("Line")
    local espText = Drawing.new("Text")

    espBox.Color = Color3.fromRGB(0, 255, 0)
    espBox.Thickness = 2
    espBox.Filled = false
    espBox.Visible = false

    espLine.Color = Color3.fromRGB(255, 255, 255)
    espLine.Thickness = 1
    espLine.Visible = false

    espText.Color = Color3.fromRGB(255, 255, 255)
    espText.Size = 16
    espText.Center = true
    espText.Outline = true
    espText.Visible = false

    game:GetService("RunService").RenderStepped:Connect(function()
        if espEnabled and target and target:FindFirstChild("HumanoidRootPart") and target:FindFirstChildOfClass("Humanoid") then
            local hrp = target.HumanoidRootPart
            local pos, onScreen = camera:WorldToViewportPoint(hrp.Position)
            local health = target:FindFirstChildOfClass("Humanoid").Health
            local maxHealth = target:FindFirstChildOfClass("Humanoid").MaxHealth
            local distance = (player.Character.HumanoidRootPart.Position - hrp.Position).Magnitude

            if onScreen then
                espBox.Visible = (espStyle == "Box")
                espLine.Visible = (espStyle == "Line")
                espText.Visible = true

                if espStyle == "Box" then
                    espBox.Size = Vector2.new(50, 75)
                    espBox.Position = Vector2.new(pos.X - 25, pos.Y - 37)
                elseif espStyle == "Line" then
                    espLine.From = Vector2.new(camera.ViewportSize.X / 2, camera.ViewportSize.Y)
                    espLine.To = Vector2.new(pos.X, pos.Y)
                end

                espText.Position = Vector2.new(pos.X, pos.Y - 50)
                espText.Text = string.format("%s\nHP: %d/%d\nDistance: %d", target.Name, health, maxHealth, math.floor(distance))
            else
                espBox.Visible = false
                espLine.Visible = false
                espText.Visible = false
            end
        else
            espBox.Visible = false
            espLine.Visible = false
            espText.Visible = false
        end
    end)
end

-- Fungsi untuk mengaktifkan ESP pada semua pemain
local function enableESP()
    for _, v in pairs(game.Players:GetPlayers()) do
        if v ~= player then
            createEsp(v.Character)
        end
    end
end

-- Toggle ESP
local ESPToggle = YourTab:CreateToggle({
    Name = "Enable ESP",
    CurrentValue = false,
    Flag = "ESPToggle",
    Callback = function(Value)
        espEnabled = Value
        if espEnabled then
            enableESP()
        end
    end
})

-- Dropdown untuk memilih player
local ESPPlayerDropdown = YourTab:CreateDropdown({
    Name = "Select Player",
    Options = {"All"},
    CurrentOption = "All",
    MultiSelection = false,
    Flag = "ESPPlayerDropdown",
    Callback = function(Value)
        selectedPlayer = Value
        enableESP()
    end
})

-- Dropdown untuk memilih bentuk ESP
local ESPStyleDropdown = YourTab:CreateDropdown({
    Name = "ESP Style",
    Options = {"Box", "Line", "Circle", "V Marker"},
    CurrentOption = "Box",
    MultiSelection = false,
    Flag = "ESPStyleDropdown",
    Callback = function(Value)
        espStyle = Value
    end
})

-- Toggle ESP untuk objek
local ESPObjectsToggle = YourTab:CreateToggle({
    Name = "Enable Object ESP",
    CurrentValue = false,
    Flag = "ESPObjectsToggle",
    Callback = function(Value)
        objectsEspEnabled = Value
        if objectsEspEnabled then
            for _, obj in pairs(workspace:GetDescendants()) do
                if obj:IsA("Tool") or obj:IsA("Part") then
                    createEsp(obj)
                end
            end
        end
    end
})

-- Auto-enable ESP setelah respawn
player.CharacterAdded:Connect(function(newChar)
    wait(1) -- Tunggu karakter benar-benar respawn
    if espEnabled then
        enableESP()
    end
end)


local player = game.Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local hum = char:WaitForChild("Humanoid")

local defaultSpeed = hum.WalkSpeed
local speedBoost = 50
local isSpeedActive = false

-- Fungsi untuk mengaktifkan speed
local function startSpeed()
    if hum then
        hum.WalkSpeed = speedBoost
    end
end

-- Fungsi untuk menghentikan speed
local function stopSpeed()
    if hum then
        hum.WalkSpeed = defaultSpeed
    end
end

-- Toggle untuk Speed Hack
local SpeedToggle = YourTab:CreateToggle({
    Name = "Enable Speed Hack",
    CurrentValue = false,
    Flag = "SpeedToggle",
    Callback = function(Value)
        isSpeedActive = Value
        if isSpeedActive then
            startSpeed()
        else
            stopSpeed()
        end
    end
})

-- Slider untuk Speed Hack
local SpeedSlider = YourTab:CreateSlider({
    Name = "Speed Amount",
    Range = {16, 100},
    Increment = 1,
    Suffix = "Speed",
    CurrentValue = speedBoost,
    Flag = "SpeedSlider",
    Callback = function(Value)
        speedBoost = Value
        if isSpeedActive then
            startSpeed()
        end
    end
})

-- Auto-enable Speed setelah respawn
player.CharacterAdded:Connect(function(newChar)
    char = newChar
    hum = char:WaitForChild("Humanoid")
    
    if isSpeedActive then
        wait(1) -- Tunggu karakter benar-benar respawn
        startSpeed()
    end
end)

-- Bypass jika game mereset WalkSpeed
task.spawn(function()
    while true do
        wait(0.1)
        if isSpeedActive and hum.WalkSpeed ~= speedBoost then
            hum.WalkSpeed = speedBoost
        end
    end
end)


-- Variables
local jumpPowerEnabled = false
local jumpPowerValue = 40

-- Function to set JumpPower
local function setJumpPower(value)
    local player = game.Players.LocalPlayer
    if player and player.Character and player.Character:FindFirstChild("Humanoid") then
        player.Character.Humanoid.UseJumpPower = true
        player.Character.Humanoid.JumpPower = value
    end
end

-- Toggle for JumpPower
TabMisc:CreateToggle({
    Name = "Enable JumpPower",
    CurrentValue = false,
    Flag = "JumpPowerToggle",
    Callback = function(value)
        jumpPowerEnabled = value
        if jumpPowerEnabled then
            setJumpPower(jumpPowerValue)
        else
            setJumpPower(40) -- Reset to default when disabled
        end
    end
})

-- Slider for JumpPower
TabMisc:CreateSlider({
    Name = "JumpPower",
    Range = {40, 1000},
    Increment = 1,
    Suffix = " power",
    CurrentValue = 40,
    Flag = "JumpPowerValue",
    Callback = function(value)
        jumpPowerValue = value
        if jumpPowerEnabled then
            setJumpPower(jumpPowerValue)
        end
    end
})


 -- Variables
local hitboxEnabled = false
local hitboxSize = 5 -- Default size

-- Function to modify hitbox size
local function setHitboxSize(size)
    for _, player in pairs(game.Players:GetPlayers()) do
        if player ~= game.Players.LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            local rootPart = player.Character.HumanoidRootPart
            rootPart.Size = Vector3.new(size, size, size)
            rootPart.Transparency = 0.7
            rootPart.Material = Enum.Material.ForceField
        end
    end
end

-- Function to reset hitbox size
local function resetHitbox()
    for _, player in pairs(game.Players:GetPlayers()) do
        if player ~= game.Players.LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            local rootPart = player.Character.HumanoidRootPart
            rootPart.Size = Vector3.new(2, 2, 1) -- Default Roblox hitbox size
            rootPart.Transparency = 0
            rootPart.Material = Enum.Material.Plastic
        end
    end
end

-- Toggle for Hitbox
TabMisc:CreateToggle({
    Name = "Enable Hitbox",
    CurrentValue = false,
    Flag = "HitboxToggle",
    Callback = function(value)
        hitboxEnabled = value
        if hitboxEnabled then
            setHitboxSize(hitboxSize)
        else
            resetHitbox()
        end
    end
})

-- Slider for Hitbox Size
TabMisc:CreateSlider({
    Name = "Hitbox Size",
    Range = {2, 50},
    Increment = 1,
    Suffix = " size",
    CurrentValue = 5,
    Flag = "HitboxSize",
    Callback = function(value)
        hitboxSize = value
        if hitboxEnabled then
            setHitboxSize(hitboxSize)
        end
    end
})



