-- Load Rayfield UI Library
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- Key System
local Key = "Ramadhan2025"
local Window = Rayfield:CreateWindow({
    Name = "Key System",
    LoadingTitle = "LoadScript...",
    LoadingSubtitle = "V3.0",
    ConfigurationSaving = { Enabled = false }
})

local KeyTab = Window:CreateTab("KeySystem", "rbxassetid://7734068326")

local KeyInput = KeyTab:CreateInput({
    Name = "Enter Key",
    PlaceholderText = "Type here...",
    RemoveTextAfterFocusLost = false,
    Callback = function(value)
        if value == Key then
            Rayfield:Notify({
                Title = "Key Verified",
                Content = "Correct Key!",
                Duration = 3
            })
            Window:DeleteTab(KeyTab)
            LoadMainUI()
        else
            Rayfield:Notify({
                Title = "Key Incorrect",
                Content = "Try Again!",
                Duration = 3
            })
        end
    end
})

-- Function to Load Main UI
function LoadMainUI()
    local MainWindow = Rayfield:CreateWindow({
        Name = "ZDZ Hub",
        LoadingTitle = "LoadScript...",
        LoadingSubtitle = "V3.0",
        ConfigurationSaving = { Enabled = false }
    })

    -- Tab Info
    local InfoTab = MainWindow:CreateTab("Info", "rbxassetid://7734068326")

    InfoTab:CreateParagraph({Title = "ZDZ Hub", Content = "This Script Made By Aan"})
    InfoTab:CreateParagraph({Title = "Release", Content = "This Script Has Release In March 5th and Year 2025. This Script Release in Ramadhan Event"})

    -- Tab Main
    local MainTab = MainWindow:CreateTab("Main", "rbxassetid://7734068326")

    -- Fly Feature
    local FlyEnabled = false
    MainTab:CreateToggle({
        Name = "Fly",
        Callback = function(state)
            FlyEnabled = state
            if FlyEnabled then
                -- Basic Fly Script
                local Player = game.Players.LocalPlayer
                local Character = Player.Character or Player.CharacterAdded:Wait()
                local HumanoidRootPart = Character:FindFirstChild("HumanoidRootPart")
                
                if HumanoidRootPart then
                    local BodyVelocity = Instance.new("BodyVelocity")
                    BodyVelocity.Velocity = Vector3.new(0, 50, 0)
                    BodyVelocity.MaxForce = Vector3.new(4000, 4000, 4000)
                    BodyVelocity.Parent = HumanoidRootPart
                    
                    while FlyEnabled do
                        task.wait()
                        HumanoidRootPart.Velocity = Vector3.new(0, 50, 0)
                    end
                    BodyVelocity:Destroy()
                end
            end
        end
    })

    -- Infinite Jump
    MainTab:CreateToggle({
        Name = "Infinite Jump",
        Callback = function(state)
            if state then
                game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
                game.Players.LocalPlayer.Character.Humanoid.UseJumpPower = true
                game:GetService("UserInputService").JumpRequest:Connect(function()
                    game.Players.LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                end)
            else
                game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
            end
        end
    })

    -- ESP (Masih Sederhana)
    MainTab:CreateToggle({
        Name = "ESP",
        Callback = function(state)
            if state then
                for _, player in pairs(game.Players:GetPlayers()) do
                    if player ~= game.Players.LocalPlayer and player.Character then
                        local Highlight = Instance.new("Highlight")
                        Highlight.Adornee = player.Character
                        Highlight.Parent = player.Character
                    end
                end
            else
                for _, player in pairs(game.Players:GetPlayers()) do
                    if player.Character then
                        for _, v in pairs(player.Character:GetChildren()) do
                            if v:IsA("Highlight") then
                                v:Destroy()
                            end
                        end
                    end
                end
            end
        end
    })

    -- Noclip
    local NoclipEnabled = false
    MainTab:CreateToggle({
        Name = "Noclip",
        Callback = function(state)
            NoclipEnabled = state
            while NoclipEnabled do
                task.wait()
                for _, part in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end
        end
    })

    -- WalkSpeed
    MainTab:CreateSlider({
        Name = "WalkSpeed",
        Min = 16,
        Max = 250,
        Default = 16,
        Callback = function(value)
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
        end
    })

    -- Gravity
    MainTab:CreateSlider({
        Name = "Gravity",
        Min = 0,
        Max = 1000,
        Default = game.Workspace.Gravity,
        Callback = function(value)
            game.Workspace.Gravity = value
        end
    })

    -- Tab Misc
    local MiscTab = MainWindow:CreateTab("Misc", "rbxassetid://7734068326")

    -- Anti Fog
    MiscTab:CreateToggle({
        Name = "Anti Fog",
        Callback = function(state)
            game.Lighting.FogEnd = state and 100000 or 1000
        end
    })

    -- Anti Lag
    MiscTab:CreateToggle({
        Name = "Anti Lag",
        Callback = function(state)
            if state then
                for _, v in pairs(game.Workspace:GetDescendants()) do
                    if v:IsA("BasePart") then
                        v.Material = Enum.Material.SmoothPlastic
                    elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
                        v.Enabled = false
                    end
                end
            end
        end
    })

    -- Full Bright
    MiscTab:CreateToggle({
        Name = "FullBright",
        Callback = function(state)
            if state then
                game.Lighting.Brightness = 5
                game.Lighting.GlobalShadows = false
            else
                game.Lighting.Brightness = 1
                game.Lighting.GlobalShadows = true
            end
        end
    })
end

Rayfield:LoadConfiguration()
